//--------------------------------------------------------------------
Static Function FSAtuSX7()
Local aEstrut   := {}
Local aAreaSX3  := SX3->( GetArea() )
Local aSX7      := {}
Local cAlias    := ""
Local nI        := 0
Local nJ        := 0
Local nTamSeek  := Len( SX7->X7_CAMPO )

AutoGrLog( STR0055 + " SX7" + CRLF ) //"Ínicio da Atualização"

aEstrut := { "X7_CAMPO", "X7_SEQUENC", "X7_REGRA", "X7_CDOMIN", "X7_TIPO", "X7_SEEK", ;
             "X7_ALIAS", "X7_ORDEM"  , "X7_CHAVE", "X7_PROPRI", "X7_CONDIC" }

//
// Campo Z2_CODMODU
//
aAdd( aSX7, { ;
	'Z2_CODMODU'															, ; //X7_CAMPO
	'001'																	, ; //X7_SEQUENC
	'SZ0->Z0_DESC'															, ; //X7_REGRA
	'Z2_MODULO'																, ; //X7_CDOMIN
	'P'																		, ; //X7_TIPO
	'S'																		, ; //X7_SEEK
	'SZ0'																	, ; //X7_ALIAS
	1																		, ; //X7_ORDEM
	'M->Z2_CODMODU'															, ; //X7_CHAVE
	'U'																		, ; //X7_PROPRI
	''																		} ) //X7_CONDIC

//
// Atualizando dicionário
//
oProcess:SetRegua2( Len( aSX7 ) )

dbSelectArea( "SX3" )
dbSetOrder( 2 )

dbSelectArea( "SX7" )
dbSetOrder( 1 )

For nI := 1 To Len( aSX7 )

	If !SX7->( dbSeek( PadR( aSX7[nI][1], nTamSeek ) + aSX7[nI][2] ) )

		AutoGrLog( STR0086 + aSX7[nI][1] + "/" + aSX7[nI][2] ) //"Foi incluído o gatilho "

		RecLock( "SX7", .T. )
		For nJ := 1 To Len( aSX7[nI] )
			If FieldPos( aEstrut[nJ] ) > 0
				FieldPut( FieldPos( aEstrut[nJ] ), aSX7[nI][nJ] )
			EndIf
		Next nJ

		dbCommit()
		MsUnLock()

		If SX3->( dbSeek( SX7->X7_CAMPO ) )
			RecLock( "SX3", .F. )
			SX3->X3_TRIGGER := "S"
			MsUnLock()
		EndIf

	EndIf
	oProcess:IncRegua2( STR0088 ) //"Atualizando Arquivos (SX7) ..."

Next nI

RestArea( aAreaSX3 )

AutoGrLog( CRLF + STR0060 + " SX7" + CRLF + Replicate( "-", 128 ) + CRLF ) //"Final da Atualização"

Return NIL


